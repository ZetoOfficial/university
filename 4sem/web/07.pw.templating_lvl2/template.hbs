<h3>{{page_name}}</h3>
<div class="container">
  <div class="row">
    {{#each blocks}}
      <div class="col-12 col-md-6 col-lg-4">
        <div class="card">
          <h5 class="card-title">{{title}}</h5>
          <p>{{content}}</p>
          <p>{{#if is_active}}Active{{else}}Inactive{{/if}}</p>
        </div>
      </div>
    {{/each}}
  </div>
</div>